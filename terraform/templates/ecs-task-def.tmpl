[
    {
        "name": "${container_name}",
        "image": "${ecr_url}/${image_name}:${image_tag}",
        "cpu": ${cpu},
        "memory": ${memory},
        "mountPoints": [],
        "portMappings": [
            {
                "protocol": "tcp",
                "containerPort": ${container_port},
                "hostPort": ${host_port}
            }
        ],
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
                "awslogs-region": "${log_region}",
                "awslogs-stream-prefix": "log",
                "awslogs-group": "/nhs/deductions/dev-327778747031/gp2gp-adaptor"
            }
        },
        "essential": true,
        "volumesFrom": [],
        "secrets": ${secrets}
    }
]